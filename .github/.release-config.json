{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"packages": {
		"typescript": {
			"release-type": "node",
			"include-v-in-tag": true,
			"include-component-in-tag": false,
			"pull-request-title-pattern": "typescript/v${version}",
			"pull-request-header": "ðŸš¢ TypeScript package release",
			"pull-request-footer": "",
			"changelog-path": "typescript/CHANGELOG.md"
		},
		"go": {
			"release-type": "go",
			"include-v-in-tag": true,
			"include-component-in-tag": true,
			"pull-request-title-pattern": "go/v${version}",
			"pull-request-header": "ðŸš¢ Go package release",
			"pull-request-footer": "",
			"changelog-path": "go/CHANGELOG.md"
		}
	},
	"changelog-sections": [
		{ "type": "feat", "section": "Features" },
		{ "type": "fix", "section": "Bug Fixes" },
		{ "type": "perf", "section": "Performance Improvements" },
		{ "type": "type", "section": "Types" },
		{ "type": "deps", "section": "Dependency Updates" },
		{ "type": "docs", "section": "Documentation" },
		{ "type": "ops", "section": "Operations" },
		{ "type": "build", "section": "Build System" },
		{ "type": "ci", "section": "Maintenance" },
		{ "type": "qol", "section": "Quality of life" },
		{ "type": "chore", "section": "Maintenance" },
		{ "type": "config", "section": "Maintenance" },
		{ "type": "refactor", "section": "Maintenance" },
		{ "type": "revert", "section": "Reverts" },
		{ "type": "security", "section": "Security" },
		{ "type": "test", "section": "Testing" },
		{ "type": "style", "section": "Code Style", "hidden": true },
		{ "type": "examples", "section": "Examples", "hidden": true }
	]
}
