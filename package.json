{
	"name": "bysquare-forked-by-bart",
	"description": "It's a national standard for payment QR codes adopted by Slovak Banking Association (SBA) - Forked version so we can use new version without type module",
	"type": "module",
	"version": "2.13.4",
	"license": "Apache-2.0",
	"homepage": "https://github.com/daaAd1/bysquare",
	"author": "Samuel Sepesi <sepesi@bart.sk>",
	"keywords": [
		"pay by square",
		"qr string",
		"paybysquare",
		"bysquare"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/daaAd1/bysquare.git"
	},
	"scripts": {
		"build": "tsc --build",
		"fmt": "dprint fmt",
		"fmt:check": "dprint check",
		"typecheck": "tsc --noEmit",
		"test": "bun test --coverage --coverage-reporter=text --coverage-reporter=lcov",
		"test:watch": "bun test --watch --coverage ."
	},
	"dependencies": {
		"lzma1": "0.2.0",
		"validator": "^13.15.15"
	},
	"devDependencies": {
		"@types/bun": "^1.2.20",
		"@types/node": "^24.2.1",
		"@types/validator": "^13.15.2",
		"dprint": "~0.50.0",
		"typescript": "~5.8.3"
	},
	"bin": "./lib/cli.js",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts",
			"require": "./lib/index.js"
		}
	},
	"files": [
		"lib/*.js",
		"lib/*.d.ts",
		"!lib/*test*"
	]
}
